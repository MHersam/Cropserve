<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarAlwaysDrawVerticalTrack="false"
    android:scrollbars="vertical">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView
            android:id="@+id/dialog_descr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:fontFamily="monospace"
            android:text="@string/dialog_description"
            android:textAlignment="center"
            android:textSize="14sp"
            android:typeface="monospace"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />


        <View
            android:id="@+id/view"
            android:layout_width="0dp"
            android:layout_height="3dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dialog_descr"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="96dp"
            android:layout_marginEnd="96dp" />

        <TextView
            android:id="@+id/ArtInfo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_feld"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="18sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="96dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="96dp"
            android:layout_marginRight="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintRight_toLeftOf="@+id/spinnerArt"/>

        <Spinner
            android:id="@+id/spinnerArt"
            android:layout_width="0dp"
            android:layout_height="28dp"
            android:background="@color/box_fill"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            app:layout_constraintRight_toRightOf="@+id/landText2"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/landText2"
            app:layout_constraintLeft_toLeftOf="@+id/landText2" />

        <TextView
            android:id="@+id/VertragInfo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_vertrag"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="18sp"
            android:typeface="normal"
            android:visibility="visible"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/ArtInfo"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/ArtInfo" />

        <Spinner
            android:id="@+id/spinnerVertrag"
            android:layout_width="0dp"
            android:layout_height="28dp"
            android:background="@color/box_fill"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/spinnerArt"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/spinnerArt"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/spinnerArt" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="34dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_region"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="18sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="16dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="33dp"
            android:layout_marginTop="23dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_stadt"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="16sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintLeft_toRightOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/view"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <AutoCompleteTextView
            android:id="@+id/landText2"
            style="@android:style/Widget.AutoCompleteTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@color/colorAccent"
            android:backgroundTint="@color/box_fill"
            android:completionHint="Region, Lage, Standort"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/bundeslandText2"
            app:layout_constraintRight_toRightOf="@+id/bundeslandText2"
            app:layout_constraintTop_toBottomOf="@+id/bundeslandText2"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_bland"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="16sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintLeft_toLeftOf="@+id/textView9"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <AutoCompleteTextView
            android:id="@+id/bundeslandText2"
            style="@android:style/Widget.AutoCompleteTextView"
            android:layout_width="186dp"
            android:layout_height="28dp"
            android:background="@color/colorAccent"
            android:backgroundTint="@color/box_fill"
            android:completionHint="Region, Lage, Standort"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="29dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/landkreisText2" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_bezirk"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="16sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintLeft_toLeftOf="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <AutoCompleteTextView
            android:id="@+id/landkreisText2"
            style="@android:style/Widget.AutoCompleteTextView"
            android:layout_width="0dp"
            android:layout_height="28dp"
            android:background="@color/colorAccent"
            android:backgroundTint="@color/box_fill"
            android:completionHint="Region, Lage, Standort"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/bundeslandText2"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/stadtText2"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/bundeslandText2" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_land"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="16sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintLeft_toLeftOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <AutoCompleteTextView
            android:id="@+id/stadtText2"
            style="@android:style/Widget.AutoCompleteTextView"
            android:layout_width="0dp"
            android:layout_height="28dp"
            android:background="@color/colorAccent"
            android:backgroundTint="@color/box_fill"
            android:completionHint="Land, Bundesland, Landkreis, Stadt, "
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/textView9"
            app:layout_constraintRight_toRightOf="@+id/landkreisText2"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/view"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="13dp"
            app:layout_constraintLeft_toLeftOf="@+id/landkreisText2" />



        <TextView
            android:id="@+id/UserInfo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/d_vnehmer"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="18sp"
            android:typeface="normal"
            android:visibility="invisible"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="32dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="@+id/spinnerUser"
            app:layout_constraintLeft_toRightOf="@+id/cancelBtn" />

        <Spinner
            android:id="@+id/spinnerUser"
            android:layout_width="168dp"
            android:layout_height="28dp"
            android:layout_marginEnd="137dp"
            android:layout_marginStart="135dp"
            android:background="@color/box_fill"
            android:visibility="invisible"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/dismissbutton"
            app:layout_constraintRight_toRightOf="@+id/dismissbutton"
            app:layout_constraintLeft_toLeftOf="@+id/UserInfo"
            app:layout_constraintTop_toTopOf="@+id/dismissbutton" />

        <Button
            android:id="@+id/cancelBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:fontFamily="sans-serif"
            android:text="@string/button_cancel"
            android:textAlignment="center"
            android:textSize="14sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.975"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="16dp"
            app:layout_constraintHorizontal_bias="0.0" />

        <Button
            android:id="@+id/dismissbutton"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:fontFamily="sans-serif"
            android:text="@string/button_okay"
            android:textAlignment="center"
            android:textSize="14sp"
            android:typeface="normal"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.975"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginRight="16dp"
            app:layout_constraintHorizontal_bias="1.0" />
    </android.support.constraint.ConstraintLayout>

</RelativeLayout>